{
    "name": "facuz/spcms",
    "description": "Sistema de administracion de paginas webs.",
    "keywords": ["web","cms", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "arcanedev/laravel-settings": "^1.1",
        "facuz/laravel-themes": "^3.1",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "htmlmin/htmlmin": "^5.6",
        "intervention/image": "^2.3",
        "laravel/dusk": "^2.0",
        "laravel/framework": "5.5.*",
        "mmamedov/page-cache": "^1.3",
        "nwidart/laravel-modules": "^2.0",
        "tremby/laravel-git-version": "^1.1",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        },
        "files": [
            "modules/Admin/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
